{
    "name": "austintoddj/canvas",
    "type": "project",
    "description": "A Laravel publishing platform",
    "keywords": [
        "blog",
        "canvas",
        "laravel",
        "publishing",
        "platform"
    ],
    "license": "MIT",
    "homepage": "https://trycanvas.app",
    "support": {
        "issues": "https://github.com/austintoddj/canvas/issues",
        "source": "https://github.com/austintoddj/canvas"
    },
    "authors": [
        {
            "name": "Todd Austin",
            "email": "austin.todd.j@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.0|^8.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "Canvas\\": "src/",
            "Canvas\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Canvas\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Canvas\\CanvasServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
